@model IEnumerable<RSWEB.Models.Student>

<h2>Students enrolled in: @ViewData["CourseTitle"]</h2>

<table class="table">
    <thead>
        <tr>
            <th>Index</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var s in Model)
        {
            <tr>
                <td>@s.StudentId</td>
                <td>@s.FirstName</td>
                <td>@s.LastName</td>
                <td><a asp-action="Details" asp-route-id="@s.Id">Details</a></td>
            </tr>
        }
    </tbody>
</table>

<a asp-controller="Courses" asp-action="Index">Back to Courses</a>
